{
  "name": "@shared/components-tests",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "npm run test:web && npm run test:native",
    "test:web": "jest --config ./jest.web.cjs",
    "test:native": "NATIVE_TESTS=true jest --config ./jest.native.cjs",
    "test:coverage": "npm run test:coverage:web && npm run test:coverage:native",
    "test:coverage:web": "jest --config ./jest.web.cjs --coverage",
    "test:coverage:native": "NATIVE_TESTS=true jest --config ./jest.native.cjs --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-react": "^7.25.0",
    "@babel/preset-typescript": "^7.25.0",
    "@supabase/supabase-js": "^2.38.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-native": "^12.9.0",
    "babel-jest": "^29.7.0",
    "babel-preset-expo": "^54.0.6",
    "expo": "~50.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-expo": "~50.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-web": "^0.21.2",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.3",
    "ws": "^8.18.3",
    "zod": "^3.22.0"
  }
}
